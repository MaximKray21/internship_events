   "source": [
    "n = 42\n",
    "answer = []\n",
    "\n",
    "for i in range(1, n + 1): # подъезды\n",
    "    for j in range(1, n + 1): # этажи\n",
    "        for k in range(1, n + 1): # квартиры\n",
    "            if i * j * k == n:\n",
    "                answer.append((i, j, k))\n",
    "print(len(answer))\n",
    "print()\n",
    "print(*answer, sep = '\\n')"
   ]
