   "source": [
    "answer = 0\n",
    "for _ in range(3):\n",
    "    x, y = map(float, input().split())\n",
    "    r = (x ** 2 + y ** 2) ** 0.5\n",
    "    if r <= 0.1:\n",
    "        answer += 3\n",
    "    elif 0.1 < r <= 0.8:\n",
    "        answer += 2\n",
    "    elif 0.8 < r <= 1:\n",
    "        answer += 1\n",
    "print(answer)    "
   ]
